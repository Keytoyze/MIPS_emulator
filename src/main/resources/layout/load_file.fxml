<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?><?import javafx.scene.control.CheckBox?><?import javafx.scene.control.Label?><?import javafx.scene.control.TextField?><?import javafx.scene.layout.AnchorPane?><?import javafx.scene.layout.VBox?>

<VBox prefHeight="275.0"
    prefWidth="403.0"
    xmlns="http://javafx.com/javafx/11.0.1"
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="indi.key.mipsemulator.controller.stage.LoadFileController">

    <AnchorPane
        minHeight="0.0"
        minWidth="0.0"
        prefHeight="323.0"
        prefWidth="403.0">

        <Label
            layoutX="25.0"
            layoutY="28.0"
            text="RAM初始化文件：" />

        <TextField
            fx:id="ramText"
            layoutX="25.0"
            layoutY="54.0"
            prefHeight="30.0"
            prefWidth="318.0" />

        <Button
            fx:id="ramButton"
            layoutX="354.0"
            layoutY="54.0"
            mnemonicParsing="false"
            prefHeight="30.0"
            text="…" />

        <TextField
            fx:id="romText"
            layoutX="24.0"
            layoutY="124.0"
            prefHeight="30.0"
            prefWidth="318.0" />

        <Button
            fx:id="romButton"
            layoutX="353.0"
            layoutY="124.0"
            mnemonicParsing="false"
            prefHeight="30.0"
            text="…" />

        <Button
            fx:id="loadButton"
            layoutX="190.0"
            layoutY="237.0"
            mnemonicParsing="false"
            text="加载" />

        <CheckBox
            fx:id="multiCheck"
            layoutX="24.0"
            layoutY="174.0"
            mnemonicParsing="false"
            selected="true"
            text="使用多周期系统" />

        <CheckBox
            fx:id="delaySlotCheck"
            layoutX="24.0"
            layoutY="203.0"
            mnemonicParsing="false"
            text="使用延迟槽（流水线系统）" />

        <Label
            fx:id="romLabel"
            layoutX="25.0"
            layoutY="98.0"
            text="ROM初始化文件：（仅单周期系统可用）" />
    </AnchorPane>
</VBox>
